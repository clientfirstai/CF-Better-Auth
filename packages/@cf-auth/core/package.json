{
  "name": "@cf-auth/core",
  "version": "0.1.0",
  "description": "Core adapter layer for CF-Better-Auth, providing compatibility and extensibility over better-auth",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "better-auth": "file:../../../vendor/better-auth",
    "@cf-auth/types": "workspace:*",
    "@cf-auth/config": "workspace:*",
    "@cf-auth/utils": "workspace:*",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT"
}