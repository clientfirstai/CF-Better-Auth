{
  "name": "@cf-auth/utils",
  "version": "0.1.0",
  "description": "Comprehensive utility functions for CF-Better-Auth ecosystem",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./crypto": {
      "types": "./dist/crypto.d.ts",
      "import": "./dist/crypto.mjs",
      "require": "./dist/crypto.js"
    },
    "./validation": {
      "types": "./dist/validation.d.ts",
      "import": "./dist/validation.mjs",
      "require": "./dist/validation.js"
    },
    "./http": {
      "types": "./dist/http.d.ts",
      "import": "./dist/http.mjs",
      "require": "./dist/http.js"
    },
    "./jwt": {
      "types": "./dist/jwt.d.ts",
      "import": "./dist/jwt.mjs",
      "require": "./dist/jwt.js"
    },
    "./storage": {
      "types": "./dist/storage.d.ts",
      "import": "./dist/storage.mjs",
      "require": "./dist/storage.js"
    },
    "./formatting": {
      "types": "./dist/formatting.d.ts",
      "import": "./dist/formatting.mjs",
      "require": "./dist/formatting.js"
    },
    "./security": {
      "types": "./dist/security.d.ts",
      "import": "./dist/security.mjs",
      "require": "./dist/security.js"
    },
    "./async": {
      "types": "./dist/async.d.ts",
      "import": "./dist/async.mjs",
      "require": "./dist/async.js"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "import": "./dist/logger.mjs",
      "require": "./dist/logger.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "import": "./dist/errors.mjs",
      "require": "./dist/errors.js"
    },
    "./events": {
      "types": "./dist/events.d.ts",
      "import": "./dist/events.mjs",
      "require": "./dist/events.js"
    },
    "./guards": {
      "types": "./dist/guards.d.ts",
      "import": "./dist/guards.mjs",
      "require": "./dist/guards.js"
    },
    "./transformers": {
      "types": "./dist/transformers.d.ts",
      "import": "./dist/transformers.mjs",
      "require": "./dist/transformers.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "import": "./dist/constants.mjs",
      "require": "./dist/constants.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@cf-auth/types": "workspace:*",
    "jose": "^5.2.0",
    "argon2": "^0.40.0",
    "zod": "^3.22.4",
    "nanoid": "^5.0.4",
    "crypto-js": "^4.2.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.1",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "@vitest/coverage-v8": "^1.1.0"
  },
  "peerDependencies": {
    "better-auth": ">=0.2.0"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "keywords": [
    "auth",
    "authentication",
    "better-auth",
    "cloudflare",
    "typescript",
    "utilities",
    "crypto",
    "jwt",
    "validation"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}